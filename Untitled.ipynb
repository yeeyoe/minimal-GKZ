   "source": [
    "A = [[0,0], [1,0], [2,0], [1,1], [0,1]] \n",
    "k = 2\n",
    "kA = [[k*x for x in point] for point in A]\n",
    "kP = Polyhedron(kA)\n",
    "kPk = kP.integral_points()  # 找到放大多面体中的整点\n",
    "Pk = [tuple(p / k for p in point) for point in kPk] \n",
    "\n",
    "#print(Pk)"
